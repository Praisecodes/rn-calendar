{
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig to read more about this file */
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "jsx": "react-native",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "declaration": true
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "./node_modules",
    "./dist"
  ]
}
